<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.changjiashuai.materialtransition">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MaterialTransition">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TransitionActivity1" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".TransitionActivity2" />
        <activity android:name=".TransitionActivity3" />
        <activity android:name=".SharedElementActivity" />
        <activity android:name=".AnimationsActivity1" />
        <activity android:name=".AnimationsActivity2"></activity>
    </application>

</manifest>